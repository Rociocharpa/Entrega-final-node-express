<h1 class="text-center mb-4">Lista de Productos</h1>
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Descripci√≥n</th>
                <th>Stock</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
            {{#each this.payload}}
            <tr>
                <td>{{this._id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.price}}</td>
                <td>{{this.description}}</td>
                <td>{{this.stock}}</td>
            </tr>
            {{/each}}
            {{/each}}
        </tbody>
    </table>
</div>
